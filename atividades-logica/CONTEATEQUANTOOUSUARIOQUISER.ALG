algoritmo "ConteateQuantoOUsuarioQuiser"
var
   valor,contador:inteiro
   
inicio
      contador <- 0
      escreva ("Quer contar ate quanto? ")
      leia(valor)
      Enquanto (contador <= valor) faca
          Escreval (contador)
          contador <- contador + 1
      FimEnquanto
      Escreval ("Terminei de contar")
fimalgoritmo